<Window x:Class="BruteForcer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BruteForcer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Start" HorizontalAlignment="Left" Margin="312,180,0,0" VerticalAlignment="Top" Height="37" Width="165" Click="Button_Click"/>
        <TextBox HorizontalAlignment="Left" Margin="312,140,0,0" Text="Threads (Number)" TextWrapping="Wrap" VerticalAlignment="Top" Width="165" TextChanged="TextBox_TextChanged" Height="35"/>
        <TextBox HorizontalAlignment="Left" Margin="312,100,0,0" Text="Password" TextWrapping="Wrap" VerticalAlignment="Top" Width="165" Height="35"/>

    </Grid>
</Window>
